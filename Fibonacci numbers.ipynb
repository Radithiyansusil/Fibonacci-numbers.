   "source": [
    "n=int(input('Number of terms in the FN series:'))\n",
    "if n==1:\n",
    "    print(0)\n",
    "elif n==2:\n",
    "    print(1)\n",
    "else:\n",
    "    i=n-2\n",
    "    k=0\n",
    "    ans=1\n",
    "    while i>0:\n",
    "        ans=ans+k\n",
    "        k=ans-k\n",
    "        i-=1\n",
    "    print(ans)"
   ]
  }
